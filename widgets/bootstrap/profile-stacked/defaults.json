{
  "styles": {
    "spm_profile-block-wrapper": {
      "background": "url(https://sailplays3.cdnvideo.ru/media/assets/assetfile/29b2150b570a651b4907beea24379aa2.png) no-repeat 30% -50px/auto 110%"
    },
    "spm_profile": {
      "font-family": "Acrom",
      "background": "url(https://sailplays3.cdnvideo.ru/media/assets/assetfile/56a5cddb28b2ce665d011358e5e52744.png) no-repeat 30% -50px/auto 110%"
    },
    "@media (max-width: 750px) | spm_profile-block-wrapper": {
      "background-position": "30% bottom"
    },
    "@media (min-width: 1678px) | spm_profile": {
      "padding": "2% 0",
      "background-size": "cover",
      "background-position": "center bottom"
    },
    "@media (max-width: 750px) | spm_profile": {
      "background-position": "30%- bottom"
    },
    "@media (max-width: 768px) | b-current-status i": {
      "width": "114vw",
      "top": "66%",
      "height": "33%"
    },
    "b-next-status": {
      "position": "relative"
    },
    "@media (max-width: 992px) | b-statuses:nth-child(2) i": {
      "width": "114vw",
      "top": "33.3%",
      "left": "-15vw",
      "height": "33.3%"
    },
    "@media (max-width: 992px) | b-statuses:nth-child(1) i": {
      "width": "114vw",
      "top": 0,
      "left": "-15vw",
      "height": "33.3%"
    },
    "points-balance": {
      "font-size": "105px",
      "line-height": "85px"
    },
    "b-points": {
      "cursor": "pointer"
    },
    "b-current-status i": {
      "right": "-4vw",
      "top": 0,
      "transform": "skew(165deg)",
      "height": "100%",
      "content": "''",
      "width": "40vw",
      "z-index": 1,
      "position": "absolute",
      "display": "block"
    },
    "b-statuses h5": {
      "color": "#7e8c8a"
    },
    "@media (max-width: 768px) | b-next-status:before": {
      "display": "none"
    },
    "b-list-close:before": {
      "right": 0,
      "bottom": 0,
      "top": 0,
      "transform": "rotate(135deg)",
      "height": "2px",
      "content": "''",
      "width": "18px",
      "background": "white",
      "position": "absolute",
      "margin": "auto",
      "display": "block",
      "left": 0
    },
    "b-statuses:nth-child(1) i": {
      "top": 0,
      "transform": "skew(165deg)",
      "height": "100%",
      "content": "''",
      "width": "120vw",
      "z-index": 1,
      "position": "absolute",
      "display": "block",
      "left": "-27vw"
    },
    "b-statuses": {
      "position": "static"
    },
    "b-statuses:nth-child(3) i": {
      "top": 0,
      "transform": "skew(165deg)",
      "height": "100%",
      "content": "''",
      "width": "120vw",
      "z-index": 2,
      "position": "absolute",
      "display": "block",
      "left": "35vw"
    },
    "@media (max-width: 992px) | b-statuses:nth-child(3) i": {
      "width": "114vw",
      "top": "66.6%",
      "left": "-15vw",
      "height": "33.3%"
    },
    "b-statuses:nth-child(2) i": {
      "right": 0,
      "border-top": "none",
      "top": 0,
      "border-left": "2px solid white",
      "transform": "skew(165deg)",
      "height": "100%",
      "content": "''",
      "width": "24vw",
      "z-index": 3,
      "border-right": "2px solid white",
      "position": "absolute",
      "border-bottom": "none",
      "margin": "auto",
      "display": "block",
      "left": "0"
    },
    "b-list-close:hover": {
      "box-shadow": "-6px 6px 14px 0 rgba(145, 70, 13, 0.66)"
    },
    "b-current-status > *": {
      "z-index": 2
    },
    "b-statuses > *": {
      "z-index": 5
    },
    "b-list-close": {
      "width": "54px",
      "top": "-27px",
      "right": 0,
      "z-index": 6,
      "height": "54px"
    },
    "b-list-close:after": {
      "right": 0,
      "bottom": 0,
      "top": 0,
      "transform": "rotate(-135deg)",
      "height": "2px",
      "content": "''",
      "width": "18px",
      "background": "white",
      "position": "absolute",
      "margin": "auto",
      "display": "block",
      "left": 0
    },
    "@media (max-width: 992px) and (min-width: 769px) | b-current-status i": {
      "width": "114vw",
      "top": "50%",
      "height": "50%"
    },
    "b-next-status:before": {
      "opacity": 0.2,
      "top": 0,
      "height": "100%",
      "content": "''",
      "width": "2px",
      "background": "#58839a",
      "position": "absolute",
      "left": "13%"
    },
    "b-current-status": {
      "cursor": "pointer",
      "position": "static"
    }
  },
